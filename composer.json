{
    "name": "biiiiiigmonster/laravel-enum",
    "description": "laravel enum helper base php version 8.1",
    "keywords": [
        "biiiiiigmonster",
        "laravel",
        "laravel-enum"
    ],
    "homepage": "https://github.com/biiiiiigmonster/laravel-enum",
    "license": "MIT",
    "authors": [
        {
            "name": "陆云峰",
            "email": "biiiiiigmonster@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BiiiiiigMonster\\LaravelEnum\\LaravelEnum\\": "src/",
            "BiiiiiigMonster\\LaravelEnum\\LaravelEnum\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BiiiiiigMonster\\LaravelEnum\\LaravelEnum\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BiiiiiigMonster\\LaravelEnum\\LaravelEnum\\LaravelEnumServiceProvider"
            ],
            "aliases": {
                "LaravelEnum": "BiiiiiigMonster\\LaravelEnum\\LaravelEnum\\Facades\\LaravelEnum"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}